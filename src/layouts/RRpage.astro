—--
import BaseLayout from “./Layout.astro”;
const { frontmatter } = Astro.props; 
—--

<BaseLayout pageTitle={frontmatter.title}>
 <h2 style="text-decoration:none;">{frontmatter.title}</h2>
<iframe src="https://docs.google.com/forms/d/e/1FAIpQLScXEETJm88Twh7r6CsFh-sv_PF0DFk3Szpme2d0O3Erpn0IAQ/viewform?embedded=true" width="640" height="1604" frameborder="0" marginheight="0" marginwidth="0">Loading…</iframe>
    <ul class="badge__list">
        <li>
            <span class="badge badge--item">AUTHOR</span>{frontmatter.author}
        </li>
        <li>
            <span class="badge badge--item">PUBLISHED</span>{frontmatter.date}
        </li>
    </ul>
    <slot />
</BaseLayout>
